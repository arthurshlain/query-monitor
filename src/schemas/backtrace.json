{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://schemas.querymonitor.com/src/schemas/backtrace.json",
	"title": "Backtrace",
	"description": "Class representing a backtrace.",
	"type": "object",
	"required": [
		"component",
		"frames"
	],
	"properties": {
		"component": {
			"phpType": "QM_Component",
			"type": "object",
			"allOf": [
				{
					"$ref": "./component.json"
				}
			]
		},
		"frames": {
			"type": "array",
			"items": {
				"type": "object",
				"allOf": [
					{
						"$ref": "./frameitem.json"
					}
				]
			}
		}
	},
	"additionalProperties": false
}
