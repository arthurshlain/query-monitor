{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://schemas.querymonitor.com/src/schemas/data/assets.json",
	"title": "Assets",
	"description": "Asset data transfer object.",
	"$comment": "/**\n * @phpstan-type Asset array{\n *   host: string,\n *   port: string,\n *   source: string|WP_Error,\n *   local: bool,\n *   ver: string,\n *   warning: bool,\n *   display: string,\n *   dependents: array<string>,\n *   dependencies: array<string>,\n * }\n * @phpstan-type AssetList array<string, Asset>\n */",
	"type": "object",
	"required": [
		"counts",
		"default_version",
		"dependencies",
		"dependents",
		"footer",
		"header",
		"full_host",
		"host",
		"is_ssl",
		"missing_dependencies",
		"port"
	],
	"properties": {
		"assets": {
			"type": "object",
			"properties": {
				"missing": {
					"type": "object",
					"phpType": "AssetList"
				},
				"broken": {
					"type": "object",
					"phpType": "AssetList"
				},
				"header": {
					"type": "object",
					"phpType": "AssetList"
				},
				"footer": {
					"type": "object",
					"phpType": "AssetList"
				}
			},
			"additionalProperties": false
		},
		"counts": {
			"type": "object",
			"properties": {
				"missing": {
					"type": "integer"
				},
				"broken": {
					"type": "integer"
				},
				"header": {
					"type": "integer"
				},
				"footer": {
					"type": "integer"
				},
				"total": {
					"type": "integer"
				}
			},
			"required": [
				"missing",
				"broken",
				"header",
				"footer",
				"total"
			],
			"additionalProperties": false
		},
		"default_version": {
			"type": "string"
		},
		"dependencies": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"dependents": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"footer": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"header": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"full_host": {
			"type": "string"
		},
		"host": {
			"type": "string"
		},
		"is_ssl": {
			"type": "boolean"
		},
		"missing_dependencies": {
			"type": "object",
			"additionalProperties": {
				"type": "boolean",
				"phpType": "true",
				"tsType": "true"
			}
		},
		"port": {
			"type": "string"
		}
	},
	"additionalProperties": false
}
