{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://schemas.querymonitor.com/src/schemas/data/doing_it_wrong.json",
	"title": "Doing_It_Wrong",
	"description": "Doing it Wrong data transfer object.",
	"type": "object",
	"required": [
		"actions"
	],
	"properties": {
		"actions": {
			"type": "array",
			"items": {
				"type": "object",
				"required": [
					"hook",
					"trace",
					"message"
				],
				"properties": {
					"hook": {
						"type": "string"
					},
					"trace": {
						"type": "object",
						"phpType": "QM_Backtrace",
						"allOf": [
							{
								"$ref": "../backtrace.json"
							}
						]
					},
					"message": {
						"type": "string"
					}
				}
			}
		}
	},
	"additionalProperties": false
}
