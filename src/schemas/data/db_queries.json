{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://schemas.querymonitor.com/src/schemas/data/db_queries.json",
	"title": "DB_Queries",
	"description": "Database queries data transfer object.",
	"$comment": "/**\n * @phpstan-type QueryRow array{\n *   caller: array<mixed>|null,\n *   caller_name: string|null,\n *   stack: array<string, mixed>,\n *   sql: string,\n *   ltime: float,\n *   result: int|bool|WP_Error,\n *   type: string,\n *   component: QM_Component|null,\n *   trace: array<string, mixed>,\n *   is_main_query: bool,\n *   filtered_trace?: array<int, array<string, mixed>>,\n * }\n */",
	"type": "object",
	"required": [
		"total_qs",
		"total_time",
		"has_result",
		"has_trace",
		"has_main_query",
		"rows",
		"errors"
	],
	"properties": {
		"total_qs": {
			"type": "integer"
		},
		"total_time": {
			"type": "number"
		},
		"errors": {
			"type": "array",
			"items": {
				"type": "object"
			}
		},
		"expensive": {
			"type": "array",
			"items": {
				"type": "object"
			}
		},
		"rows": {
			"phpStanType": "array<int, QueryRow>",
			"type": "array",
			"items": {
				"type": "object",
				"required": [
					"caller",
					"caller_name",
					"stack",
					"sql",
					"ltime",
					"result",
					"type",
					"component",
					"trace",
					"is_main_query"
				],
				"properties": {
					"caller": {
						"type": [
							"object",
							"null"
						]
					},
					"caller_name": {
						"type": [
							"string",
							"null"
						]
					},
					"stack": {
						"type": "object"
					},
					"sql": {
						"type": "string"
					},
					"ltime": {
						"type": "number"
					},
					"result": {
						"oneOf": [
							{
								"type": "integer"
							},
							{
								"type": "boolean"
							},
							{
								"type": "object",
								"tsType": "WP_Error",
								"phpType": "WP_Error",
								"allOf": [
									{
										"$ref": "../../../node_modules/wp-json-schemas/schemas/error.json"
									}
								]
							}
						]
					},
					"type": {
						"type": "string"
					},
					"component": {
						"oneOf": [
							{
								"phpType": "QM_Component",
								"type": "object",
								"allOf": [
									{
										"$ref": "../component.json"
									}
								]
							},
							{
								"type": "null"
							}
						]
					},
					"trace": {
						"type": "object"
					},
					"is_main_query": {
						"type": "boolean"
					},
					"filtered_trace": {
						"phpType": "array<int, array<string, mixed>>",
						"type": "array",
						"items": {
							"allOf": [
								{
									"$ref": "../frameitem.json"
								}
							]
						}
					}
				},
				"additionalProperties": false
			}
		},
		"has_result": {
			"type": "boolean"
		},
		"has_trace": {
			"type": "boolean"
		},
		"has_main_query": {
			"type": "boolean"
		},
		"times": {
			"type": "object",
			"additionalProperties": {
				"type": "object",
				"properties": {
					"caller": {
						"type": "string"
					},
					"ltime": {
						"type": "number"
					},
					"types": {
						"type": "object",
						"additionalProperties": {
							"type": "integer"
						}
					}
				},
				"required": [
					"caller",
					"ltime",
					"types"
				],
				"additionalProperties": false
			}
		},
		"dupes": {
			"type": "object",
			"additionalProperties": {
				"type": "array",
				"items": {
					"type": "integer"
				}
			}
		}
	},
	"additionalProperties": false
}
