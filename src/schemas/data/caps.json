{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://schemas.querymonitor.com/src/schemas/data/caps.json",
	"title": "Caps",
	"description": "User capability checks data transfer object.",
	"type": "object",
	"required": [
		"caps",
		"parts",
		"users",
		"components"
	],
	"properties": {
		"caps": {
			"type": "array",
			"items": {
				"type": "object",
				"required": [
					"args",
					"trace",
					"result",
					"parts",
					"name",
					"user"
				],
				"properties": {
					"args": {
						"type": "array"
					},
					"trace": {
						"type": "object",
						"phpType": "QM_Backtrace",
						"allOf": [
							{
								"$ref": "../backtrace.json"
							}
						]
					},
					"result": {
						"type": "boolean"
					},
					"parts": {
						"type": "array",
						"items": {
							"type": "string"
						}
					},
					"name": {
						"type": "string"
					},
					"user": {
						"type": "string"
					}
				},
				"additionalProperties": false
			}
		},
		"parts": {
			"type": "array",
			"items": {
				"type": "string"
			}
		},
		"users": {
			"type": "array",
			"items": {
				"type": "integer"
			}
		},
		"components": {
			"type": "object",
			"additionalProperties": {
				"type": "string"
			}
		}
	},
	"additionalProperties": false
}
