{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://schemas.querymonitor.com/src/schemas/data/transients.json",
	"title": "Transients",
	"description": "Transients data transfer object.",
	"type": "object",
	"required": [
		"trans",
		"has_type"
	],
	"properties": {
		"trans": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string"
					},
					"trace": {
						"type": "object",
						"phpType": "QM_Backtrace",
						"allOf": [
							{
								"$ref": "../backtrace.json"
							}
						]
					},
					"type": {
						"type": "string",
						"enum": [
							"blog",
							"site"
						]
					},
					"value": {},
					"expiration": {
						"type": "integer"
					},
					"exp_diff": {
						"type": "string"
					},
					"size": {
						"type": "integer"
					},
					"size_formatted": {
						"type": "string"
					}
				},
				"required": [
					"name",
					"trace",
					"type",
					"value",
					"expiration",
					"exp_diff",
					"size",
					"size_formatted"
				],
				"additionalProperties": false
			}
		},
		"has_type": {
			"type": "boolean"
		}
	},
	"additionalProperties": false
}
