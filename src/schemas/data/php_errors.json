{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "https://schemas.querymonitor.com/src/schemas/data/php_errors.json",
	"title": "PHP_Errors",
	"description": "PHP errors data transfer object.",
	"type": "object",
	"definitions": {
		"errorObject": {
			"type": "object",
			"properties": {
				"errno": {
					"type": "integer"
				},
				"level": {
					"type": "string",
					"enum": [
						"warning",
						"notice",
						"strict",
						"deprecated"
					]
				},
				"suppressed": {
					"type": "boolean"
				},
				"message": {
					"type": "string"
				},
				"file": {
					"type": [
						"string",
						"null"
					]
				},
				"filename": {
					"type": "string"
				},
				"line": {
					"type": [
						"integer",
						"null"
					]
				},
				"trace": {
					"type": "object",
					"phpType": "QM_Backtrace",
					"allOf": [
						{
							"$ref": "../backtrace.json"
						}
					]
				},
				"count": {
					"type": "integer"
				}
			},
			"required": [
				"errno",
				"level",
				"suppressed",
				"message",
				"file",
				"filename",
				"line",
				"count"
			],
			"additionalProperties": false
		}
	},
	"properties": {
		"errors": {
			"type": "object",
			"additionalProperties": {
				"type": "object",
				"$ref": "#/definitions/errorObject"
			}
		}
	},
	"additionalProperties": false
}
